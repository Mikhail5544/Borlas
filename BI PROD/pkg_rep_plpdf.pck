        plpdf.putimage(p_name => 'image_sign_jpg'
                      ,p_data => v_signer.image_sign
                      ,p_x    => plpdf.getpagespace - 5 - 45
                      ,p_y    => plpdf.getcurrenty
                      ,p_w    => 45
                      ,p_h    => 50);
      END IF;
    
      IF par_is_detailed
      THEN
        plpdf.printmultilinecell(p_txt      => 'Полисные условия к договору страхования жизни по программе «' ||
