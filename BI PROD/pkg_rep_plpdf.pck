        plpdf.printcell(p_border => 'TB', p_w => 5, p_ln => 0);
      
        plpdf.printcell(p_h      => -1
                       ,p_w      => plpdf.gettextwidth(rec.name) + 5
                       ,p_txt    => rec.name
                       ,p_border => 'TB'
                       ,p_ln     => 0);
      END LOOP;
    
      plpdf.printcell(p_ln => 1, p_border => 'BTR');
      /*Двойная линия*/
      line;
    
    END print_currency;
  
