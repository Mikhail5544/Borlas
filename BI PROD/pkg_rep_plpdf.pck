  CURSOR cur_tmp_bso(par_number_of_copies INTEGER) IS
    SELECT /*+ ORDERED USE_NL(t,b)*/
     t.bso_id
    ,bs.proposal_valid_days
    ,pkg_xx_pol_ids.cre_new_ids(bs.series_num, b.num) ids
      FROM tmp_bso_to_print t
