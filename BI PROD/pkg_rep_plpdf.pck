    PROCEDURE prn
    (
      par_text VARCHAR2
     ,par_ln   NUMBER DEFAULT 1
     ,par_w    NUMBER DEFAULT 0
    ) IS
    BEGIN
      plpdf.printmultilinecell(p_ln       => par_ln
                              ,p_align    => 'J'
