                ,par_style       => gc_style_regular
                ,par_font_family => gc_barcode_font_family_code39);
        plpdf.printcell(p_h => -1, p_txt => v_barcode, p_align => 'R', p_ln => 1);
      END IF;
    END print_header;
  
  BEGIN
    v_policy_id := to_number(repcore.get_context('POL_ID'));
  
    BEGIN
      SELECT nvl(ids, 0)
            ,payment_terms_desc
            ,period_desc
            ,assured_contact_id
