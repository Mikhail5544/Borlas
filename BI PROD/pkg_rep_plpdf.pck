                        ,rownum rn
                    FROM t_prod_line_option plo
                        ,t_product_line pl
                        ,v_prod_product_line ppl
                        ,t_product_line_type plt
                        ,(SELECT pc.t_prod_line_option_id
                                ,pc.ins_amount
