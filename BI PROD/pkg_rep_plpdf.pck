    PROCEDURE print_payment_terms IS
      v_others_payment_date VARCHAR2(500);
    BEGIN
      set_font(par_style => gc_style_bold);
      plpdf.printcell(p_w   => plpdf.getpagespace / 2
                     ,p_txt => 'ДАТА УПЛАТЫ ПЕРВОГО СТРАХОВОГО ВЗНОСА: '
