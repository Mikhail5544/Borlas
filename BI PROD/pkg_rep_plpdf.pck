                                  ,crt.relationship_dsc
                                  ,ab.value
                                  ,row_number() over(ORDER BY ab.as_beneficiary_id) rn
                              FROM as_beneficiary     ab
                                  ,t_contact_rel_type crt
                                  ,cn_contact_rel     ccr
                                  ,contact            c
                                  ,cn_person          cp
