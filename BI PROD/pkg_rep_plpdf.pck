    PROCEDURE print_programs_custom IS
      v_total_fee NUMBER := 0;
    BEGIN
      set_font;
      delete_row_print_cache;
    
      gv_widths(1) := plpdf.getpagespace / 6 * 4;
      gv_widths(2) := plpdf.getpagespace / 6;
