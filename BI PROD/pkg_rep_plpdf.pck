              'филиале / агентстве Компании.';
    plpdf.printmultilinecell(p_txt => v_body, p_h => -1, p_w => 190, p_ln => 1);
  
    -------------------------------------------------------------------------------------------------
    -- ПОДПИСИ
    -------------------------------------------------------------------------------------------------
    blank_cell(par_width => 0, par_height => 10, par_ln => 1);
  
