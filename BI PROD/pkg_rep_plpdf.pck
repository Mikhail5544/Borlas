                    FROM t_product       p
                        ,t_prod_currency pc
                        ,fund            f
                   WHERE p.product_id = pc.product_id
                     AND pc.currency_id = f.fund_id
                     AND p.product_id = v_pol_sum.product_id
                     AND f.brief = 'RUR' /*Печатаем только рубли*/
                   ORDER BY pc.is_default DESC NULLS LAST
                           ,f.fund_id     DESC)
